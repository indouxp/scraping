YAHOO.util.Event.onDOMReady(function () {

  var expandable   = 1;
  var collapseable = 0;

  function canExpand(val) {
    if (val == 0) {
      if (expandable == 1) {
        $("#expandAll").css("display","none");
        $("#expandAllText").css("display","");
      }
    } else {
      if (expandable == 0) {
        $("#expandAll").css("display","");
        $("#expandAllText").css("display","none");
      }
    }
    expandable = val;
  }

  function canCollapse(val) {
    if (val == 0) {
      if (collapseable == 1) {
        $("#collapseAll").css("display","none");
        $("#collapseAllText").css("display","");
      }
    } else {
      if (collapseable == 0) {
        $("#collapseAll").css("display","");
        $("#collapseAllText").css("display","none");
      }
    }
    collapseable = val;
  }

  // define activity product toggle functions.
  var expandImg = '<img src="https://images-na.ssl-images-amazon.com/images/G/01/webservices/ja/icon_expand._V174119381_.gif" width="14" height="14" border="0" />';
  var collapseImg = '<img src="https://images-na.ssl-images-amazon.com/images/G/01/webservices/ja/icon_collapse._V174119389_.gif" width="14" height="14" border="0" />';

  function expandProduct(element) {
    var elementId = $(element).attr("id");
    var relatedElementsId = elementId.replace("toggle_","");
    $("#activity_table_block").find("*[id^="+relatedElementsId+"]").css("display","");
    $("#"+elementId+" img").replaceWith(collapseImg);
  }

  function collapseProduct(element) {
    var elementId = $(element).attr("id");
    var relatedElementsId = elementId.replace("toggle_","");
    $("#activity_table_block").find("*[id^="+relatedElementsId+"]").css("display","none");
    $("#"+elementId+" img").replaceWith(expandImg);
  }

  $("#activity_table_block").find("a[id^=toggle_]").toggle(function() {
    expandProduct(this);
    canCollapse(1);
  }, function() {
    collapseProduct(this);
    canExpand(1);
  });

  function expandAll() {
    $("#activity_tab_block").find("a[id^=toggle_]").each(function() {
      expandProduct(this);
    });
    canCollapse(1);
    canExpand(0);
  }
 
  function collapseAll() {
    $("#activity_tab_block").find("a[id^=toggle_]").each(function() {
      collapseProduct(this);
    });
    canCollapse(0);
    canExpand(1);
  }

  $("#expandAll a").click(function() { expandAll()});

  $("#collapseAll a").click(function() { collapseAll()});
});

