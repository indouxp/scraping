<script type="text/javascript">

// Invoice ajax logic below
YAHOO.namespace("invoiceCheckRequestId");
YAHOO.namespace("invoiceCheckBaseUrl");
YAHOO.namespace("invoiceCheckDestUrl");
YAHOO.namespace("invoiceCheckChargeId");
YAHOO.namespace("invoiceCheckInterval");
YAHOO.namespace("invoiceCheckAttempt");
YAHOO.namespace("invoiceCheckMaxAttempt");
YAHOO.namespace("invoiceCheckSuccess");
YAHOO.namespace("invoiceCheckMinSuccess");

YAHOO.invoiceCheckInterval = 100;
YAHOO.invoiceCheckMaxAttempt = 30;
YAHOO.invoiceCheckMinSuccess = 1;

function scheduleInvoiceCheck() {
    YAHOO.invoiceCheckRequestId = setTimeout(invoiceCheck, YAHOO.invoiceCheckInterval);
}

var handleInvoiceEvent = {
  invoiceValidateSuccess:function(eventType, args){
    var baseUrl = YAHOO.invoiceCheckBaseUrl;
    var destUrl = YAHOO.invoiceCheckDestUrl;
    var o = args[0];
    if(o.responseText !== undefined){
      if(o.getResponseHeader['success-error-status'] == 500) {
        hideChargeSpinner(YAHOO.invoiceCheckChargeId);
        showSystemError();
      }
      else {
        var invoiceStatus; 
        try {
          invoiceStatus = YAHOO.lang.JSON.parse(o.responseText);
          if (invoiceStatus.streamExists == 1) {
            if (YAHOO.invoiceCheckSuccess < YAHOO.invoiceCheckMinSuccess) {
                // S3 consistency can be a problem, give settle time / validate >1
                YAHOO.invoiceCheckSuccess = YAHOO.invoiceCheckSuccess + 1;
                scheduleInvoiceCheck();
            } else {
                hideChargeSpinner(YAHOO.invoiceCheckChargeId);
                window.frames["downloader"].location = destUrl;
            }
          } else {
            YAHOO.invoiceCheckAttempt = YAHOO.invoiceCheckAttempt + 1;
            YAHOO.invoiceCheckSuccess = 0;
            if (YAHOO.invoiceCheckAttempt < YAHOO.invoiceCheckMaxAttempt) {
                scheduleInvoiceCheck();
            } else {
                hideChargeSpinner(YAHOO.invoiceCheckChargeId);
                showError("エラーが発生しました", "処理中にエラーが発生しました", "現在リクエストされた請求書を発行できません。しばらくしてからもう一度試してください。" );
            }
          }
        }
        catch(e) {
          showSystemError();
        }
        
      }
    }
  },
  failure:function(eventType, args){
    showSystemError();
  }
};

function invoiceCheck() {
    var checkUrl = YAHOO.invoiceCheckBaseUrl + "&attempt=" + YAHOO.invoiceCheckAttempt;
    var callback = {
    customevents:{
      onSuccess:handleInvoiceEvent.invoiceValidateSuccess,
      onFailure:handleInvoiceEvent.failure
    },
    cache:false,
    scope:handleInvoiceEvent
  };

  YAHOO.util.Connect.initHeader("portal-ajax-request","191-0852350-5994913");
  var request = YAHOO.util.Connect.asyncRequest('GET', checkUrl, callback, null);
  
  return false;
};

function showChargeSpinner(chargeId) {
    $('#pdfImage' + chargeId).hide();
    $('#invoiceSpinner' + chargeId).show();
};

function hideChargeSpinner(chargeId) {
    $('#pdfImage' + chargeId).show();
    $('#invoiceSpinner' + chargeId).hide();
};

function invoiceClicked(chargeId, baseUrl, destUrl) {
    showChargeSpinner(chargeId);

    YAHOO.invoiceCheckChargeId = chargeId;
    YAHOO.invoiceCheckBaseUrl = baseUrl;
    YAHOO.invoiceCheckDestUrl = destUrl;
    YAHOO.invoiceCheckAttempt = 0;
    YAHOO.invoiceCheckSuccess = YAHOO.invoiceCheckMinSuccess;

    invoiceCheck();

